{
   "name": "react-test-bench",
   "version": "1.0.0",
   "description": "React Test Bench for React development",
   "main": "server.js",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "node server.js",
      "build": "webpack --mode development",
      "analyzer": "npm run generate-webpack-stats && npm run start-analyze-server",
      "generate-webpack-stats": "node_modules/.bin/webpack --profile --json > stats.json",
      "start-analyze-server": "node_modules/.bin/webpack-bundle-analyzer stats.json",
      "postinstall": "webpack -p --config webpack.prod.config.js"
   },
   "author": "NeoAren",
   "license": "MIT",
   "devDependencies": {
      "@babel/core": "^7.4.4",
