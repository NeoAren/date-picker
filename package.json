      "express": "^4.17.1"
   },
   "devDependencies": {
      "@babel/core": "^7.4.5",
      "@babel/preset-env": "^7.4.5",
      "@babel/preset-react": "^7.0.0",
      "babel-eslint": "^10.0.1",
      "babel-loader": "^8.0.6",
      "babel-polyfill": "^6.26.0",
      "css-loader": "^2.1.1",
      "eslint": "^5.16.0",
