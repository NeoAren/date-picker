      "babel-eslint": "^10.0.1",
      "babel-loader": "^8.0.6",
      "babel-polyfill": "^6.26.0",
      "css-loader": "^2.1.1",
      "eslint": "^5.16.0",
      "eslint-loader": "^2.1.2",
      "node-sass": "^4.12.0",
      "react": "^16.8.6",
      "react-dom": "^16.8.6",
      "react-redux": "^6.0.1",
      "react-router-dom": "^4.3.1",
      "redux": "^4.0.1",
      "sass-loader": "^7.1.0",
      "style-loader": "^0.23.1",
      "webpack": "^4.32.0",
      "webpack-bundle-analyzer": "^3.3.2",
      "webpack-cli": "^3.3.2",
      "webpack-dev-middleware": "^3.7.0",
      "webpack-dev-server": "^3.4.1",
      "webpack-hot-middleware": "^2.25.0"
   },
   "dependencies": {
      "body-parser": "^1.19.0",
      "date-fns": "^1.30.1",
      "express": "^4.17.0",
      "image-size": "^0.6.3",
      "jsonwebtoken": "^8.5.1",
      "markdown-it": "^8.4.2",
      "markdown-it-checkbox": "^1.1.0",
      "markdown-it-deflist": "^2.0.3",
