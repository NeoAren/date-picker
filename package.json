{
   "name": "neo-date-picker",
   "version": "1.0.0",
   "description": "A modern and easy-to-use React date-picker component",
   "main": "server.js",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/NeoAren/neo-date-picker.git"
   },
   "author": "NeoAren",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/NeoAren/neo-date-picker/issues"
   },
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "node server.js",
      "build": "webpack --mode development",
